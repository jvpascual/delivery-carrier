        <template id="TransmEtiquetaEnvio">
            <soapenv:Envelope
                xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
                xmlns:mrw="http://www.mrw.es/">
                <soapenv:Header>
                    <mrw:AuthInfo>
                        <mrw:CodigoFranquicia>
                            <t t-esc="franchise"/>
                        </mrw:CodigoFranquicia>
                        <mrw:CodigoAbonado>
                            <t t-esc="subscriber"/>
                        </mrw:CodigoAbonado>
                        <mrw:CodigoDepartamento>
                            <t t-esc="department"/>
                        </mrw:CodigoDepartamento>
                        <mrw:UserName>
                            <t t-esc="username"/>
                        </mrw:UserName>
                        <mrw:Password>
                            <t t-esc="password"/>
                        </mrw:Password>
                    </mrw:AuthInfo>
                </soapenv:Header>
                <soapenv:Body>
                    <mrw:GetEtiquetaEnvio>
                        <mrw:request>
                            <mrw:NumeroEnvio>
                                <t t-esc="delivery_number"/>
                            </mrw:NumeroEnvio>
                            <mrw:SeparadorNumerosEnvio>
                                <t t-esc="delivery_number_split"/>
                            </mrw:SeparadorNumerosEnvio>
                            <mrw:FechaInicioEnvio>
                                <t t-esc="date_start"/>
                            </mrw:FechaInicioEnvio>
                            <mrw:FechaFinEnvio>
                                <t t-esc="date_end"/>
                            </mrw:FechaFinEnvio>
                            <mrw:TipoEtiquetaEnvio>
                                <t t-esc="sticker_type"/>
                            </mrw:TipoEtiquetaEnvio>
                            <mrw:ReportTopMargin>
                                <t t-esc="top_margin"/>
                            </mrw:ReportTopMargin>
                            <mrw:ReportLeftMargin>
                                <t t-esc="left_margin"/>
                            </mrw:ReportLeftMargin>
                        </mrw:request>
                    </mrw:GetEtiquetaEnvio>
                </soapenv:Body>
            </soapenv:Envelope>
        </template>

